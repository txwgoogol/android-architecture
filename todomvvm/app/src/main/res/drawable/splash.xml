<?xml version="1.0" encoding="utf-8"?>

<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
	
	<item>
		<shape>
			<solid android:color="@color/windowBackground" />
		</shape>
	</item>
	
	<item android:bottom="60dp">
		<bitmap
